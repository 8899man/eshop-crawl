<div class="list-div" data-time="<%= wine_monitor.updated_at.to_i %>">
  <div class="pull-left list-detail"> 
    <%= link_to wine_monitor.name.try(:block,150), wine_monitor,target: '_blank', title: wine_monitor.name %>
    <p><b>现价</b>：<%= link_to wine_monitor.current_price, wine_monitor,target: '_blank', title: wine_monitor.name %></p>
    <p><b>最低价</b>：<%= link_to wine_monitor.min_price, wine_monitor, target: '_blank', title: wine_monitor.name %></p>
  </div>
  <div class="clearfix"></div>
</div>


